@model List<Employee>
@{
    ViewData["Title"] = "Show All Employee"; //set  layout
}
<a asp-action="New" asp-controller="Employee">New</a>
<h1>Index</h1>
<table class="table table-hover table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th></th>
    </tr>
    @foreach (Employee item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>
                <a href="/Employee/Edit/@item.Id">Edit</a>
            </td>
        </tr>
    }

</table>



