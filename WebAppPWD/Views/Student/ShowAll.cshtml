

<table class="table table-hover tab-border">
   @foreach (Student item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.ImageURL</td>
            <td>
                <img src="~/Images/@item.ImageURL" style="width:50px"/>
            </td>
        </tr>
    }
</table>

